<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.ListGreenSpaceUI">
   <children>
      <GridPane layoutX="148.0" layoutY="99.0" prefHeight="53.0" prefWidth="312.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="lblSelectAlgorithm" prefHeight="17.0" prefWidth="156.0" text="Select a sorting algorithm" />
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
               <children>
                  <CheckBox fx:id="optDefault" mnemonicParsing="false" prefHeight="31.0" prefWidth="73.0" text="Default" />
                  <CheckBox fx:id="optAlternative" mnemonicParsing="false" text="Alternative" />
               </children>
            </VBox>
         </children>
      </GridPane>
      <Button fx:id="btnConfirm" layoutX="274.0" layoutY="161.0" mnemonicParsing="false" onAction="#handleConfirm" text="Confirm" />
      <Label fx:id="lblOut" layoutX="172.0" layoutY="200.0" prefHeight="100.0" prefWidth="265.0" />
   </children>
</AnchorPane>
